{
  "name": "vue-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve",
    "prod": "vue-cli-service build --mode production",
    "stage": "vue-cli-service build --mode development"
  },
  "dependencies": {
    "chart.js": "^2.9.3",
    "core-js": "^3.4.8",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "vue": "^2.6.10",
    "vue-cropperjs": "^4.0.1",
    "vue-i18n": "^8.15.1",
    "vue-multiselect": "^2.1.6",
    "vue-password-strength-meter": "^1.6.1",
    "vue-router": "^3.1.3",
    "vue2-daterange-picker": "^0.4.4",
    "vuejs-paginate": "^2.1.0",
    "vuex": "^3.1.2",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off",
      "no-unreachable": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "globals": {
      "$": true
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
